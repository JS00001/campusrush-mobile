{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": false,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@/types": ["./src/@types"],
      "@/types/*": ["./src/@types/*"]
    }
  },
  "exclude": ["node_modules", "babel.config.js", "metro.config.js"]
}
